{
  "name": "local-llm-vscode",
  "displayName": "walmart-copilot",
  "description": "code complete using local llms",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "walmart-copilot.helloWorld",
        "title": "wc: hello world"
      },
      {
        "command": "walmart-copilot.startServer",
        "title": "wc: start Ollama"
      },
      {
        "command": "walmart-copilot.autocomplete",
        "title": "wc: autocomplete"
      }
    ],
    "keybindings": [
      {
        "key": "cmd+shift+l",
        "command": "editor.action.inlineSuggest.trigger"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "ollama": "^0.3.0"
  }
}
